"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/demo/tamoshanter"

	"$bumpmap" "models/effects/flat_normal" 

	

	"$yellow" "0"

	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" ".05"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	
 	"$blendtintbybasealpha" "1" 
	"$blendtintcoloroverbase" "0"   // between 0-1 determines how much blended by tinting vs. replacing the color

	"$colortint_base" "{155 31 33}"  // put the RGB value of whats being colored when no paint is present, if $blendtintcoloroverbase is 0 then put [1 1 1] here.
	"$colortint_tmp" "[0 0 0]" 

	"$rimlight" "1"
	"$rimlightexponent" "2"
	"$rimlightboost" "1"

	// Cloaking
	
	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}