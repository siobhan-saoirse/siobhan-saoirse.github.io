"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/demo/top_hat"
	"$bumpmap" "models/player/items/demo/top_hat_normal" 
	
	"$yellow" "0"

	"$blendtintbybasealpha" "1" 
	"$blendtintcoloroverbase" "1"   // between 0-1 determines how much blended by tinting vs. replacing the color

	"$colortint_base" "{59 49 48}"
	"$colortint_tmp" "[0 0 0]"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" ".6"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[1 2 4]"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" ".65"

	// Cloaking
	
	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}