"VertexLitGeneric"
{
	"$basetexture"	"models/player/items/crafting/halloween_case_spider"
	"$basemapalphaphongmask" 1
	
	"$envmap" "cubemaps\cubemap_specular001"
	"$envmaptint" "[.15 .13 .1]"
	"$yellow" "0"
	
	"$blendtintbybasealpha" "0" 
	"$blendtintcoloroverbase" "1"
	
//	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	
	//phong properties
	"$phong"	"1"
	"$phongboost"	".01"
	"$phongexponent"	"20"
	"$phongfresnelranges"	"[.5 1 2]"
	"$phongtint"  "[2 .4 .1]"
	"$halflambert" "0"
	
	"$selfillum" 1	
	"$selfillumtint" "[3 1.5 2]"
    "$selfIllumFresnel" "1"
    "$selfIllumFresnelMinMaxExp" "[0 10 20]" // 0.0 1.0 1.0 is the default
	"$colortint_base" "{158 156 160}"
	"$colortint_tmp" "[0 0 0]"
	"Proxies"
	{	
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}