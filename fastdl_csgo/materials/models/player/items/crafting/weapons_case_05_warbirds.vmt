"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/crafting/weapons_case_05_warbirds"
	"$bumpmap" "models/effects/flat_normal" 

	"$phong" "1"

	"$phongboost" "1.25"
	"$phongalbedotint" "1"

	//scales up the exponent texture which usually only has about 120 levels of exponent
	"$phongexponentfactor"  "1000"

	"$lightwarptexture" "models/lightwarps/softened_weapon_lightwarp"

	"$phongfresnelranges"	"[.2 2.5 .5]"
	"$halflambert" "1"
	"$glowcolor" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.1 .1 .1]"

	"$selfillum" 1
	"$selfillumtint" "[1.1 .96 .7]"
	"$selfIllumFresnel" "1"
	"$selfIllumFresnelMinMaxExp" "[-.5 7 15]"

	"$phongtint" "[.6 .5 .5]"

	"$tempvar" "0"

	//commented out to use the exponent mask
	//"$basephongexponent" "15"
	//"$basephongboost" "3"

	"$basemapalphaphongmask" "1"

	"$rimlight" "1"
	"$rimlightexponent" "2"
	"$rimlightboost" "1.0"
	"$rimmask" "1"

	// Cloaking
	
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
		"WeaponSkin" {
		}
	}
}