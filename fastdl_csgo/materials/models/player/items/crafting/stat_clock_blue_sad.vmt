"UnlitTwoTexture"
{
	"$baseTexture" "models/player/items/crafting/stat_clock_blue_sad"
	"$texture2"  "models/weapons/c_items/stattracker_lines"
	
	$basealphaenvmapmask "1"
	$envmap 		"env_cubemap"
	$envmaptint		"[0.4 0.4 0.4]"

	"$selfillum" "1"

	"$lightwarptexture" "models\buildables\sentry1\Sentry1_lightwarp"
	
	"$rimlight" "1"		
	"$rimlightexponent" "4"				
	"$rimlightboost" "1.5"
	
	"$glowcolor" "1"
	
	"$txScale" "1"

	"$yellow" "0"

	"Proxies"
	{	
		"StatTrakIcon"
		{
			"resultVar" "$basetexturetransform"
		}
		"TextureScroll"
		{
		"texturescrollvar" "$texture2transform"
		"texturescrollrate" -0.075
		"texturescrollangle" 90
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}	
	}
}
