"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/all_class/parasite_skin"
	"$bumpmap" "models/player/items/all_class/parasite_skin_phongmask" 
	
	"$yellow" "0"

	"$phong" "1"
	"$phongexponent" "8"
	"$phongboost" "3"
	//"$lightwarptexture" "models/player/pyro/pyro_lightwarp.vtf"	
	"$phongfresnelranges"	"[.25 1 4]"

 	"$blendtintbybasealpha" "1" 
 	"$blendtintcoloroverbase" "0"  

	"$colortint_base" "{184 196 56}"
	"$colortint_tmp" "[0 0 0]" 

	//"$envmap" "env_cubemap"
	//"$envmaptint" "[.4 .4 .4]"
	//"$basealphaenvmapmask" 1 


	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "10"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "1"										// Boost for ambient cube component of rim lighting

	// Cloaking
	

	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}