"VertexlitGeneric"
{
	"$baseTexture" "models/player/items/all_class/stampable_medal_decal"	
	
	"$yellow" "0"

	"$basemapalphaphongmask"	"1"

	"$phong" "1"
	"$phongexponent" "8"
	"$phongboost" "5"
	"$phongfresnelranges" "[.25 1 4]"
		
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"

	"$rimlight" "1"
	"$rimlightexponent" "4"
	"$rimlightboost" "1"

	">=DX90"
	{
		"$envmap" "env_cubemap"
		"$envmaptint" "[1.2 1.2 1.9]"
	}

	// Cloaking
	
	"Proxies"
	{
		"CustomSteamImageOnModel"
		{
		}
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}