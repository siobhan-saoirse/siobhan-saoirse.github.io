"VertexlitGeneric"
{
	"$basetexture" "models/player/items/all_class/hm_duck"
	"$bumpmap" "models/player/items/all_class/hm_duck_normal"

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" "0.6"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"

	"$phongfresnelranges"	"[.25 1 4]"

	"$yellow" "0"

	"$blendtintbybasealpha" "1" 
	"$blendtintcoloroverbase" "0"   // between 0-1 determines how much blended by tinting vs. replacing the color

	"$colortint_base" "{239 161 79}"  // put the RGB value of whats being colored when no paint is present, if $blendtintcoloroverbase is 0 then put [1 1 1] here.
	"$colortint_tmp" "[1 1 1]"
	"$color2" "{239 161 79}"






	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "9"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "1"	
	// Cloaking
	
	"Proxies"
	{
		"invis"
		{
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}