"VertexLitGeneric"
{
	"$baseTexture" "models/player/items/mvm_loot/spy/chain_spy"
	$BASEMAPALPHAPHONGMASK 1

	"$normalmapalphaenvmapmask" 1
	"$envmap" "env_cubemap"
	"$envmaptint" "[.35 .35 .35]"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$tempvar" "0"

	"$basephongexponent" "25"
	"$basephongboost" "5"

	"$commadd_phongexponent" "30"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"


	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "2"

	"$glowcolor" "1"

	// Cloaking
	
	

	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"CommunityWeapon"
		{
			"resultVar" "$commweapon"
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongexponent" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongexponent" 
			"resultVar" "$phongexponent" 
		}
		"Multiply"
		{
			"srcVar1"  "$commweapon"
			"srcVar2"  "$commadd_phongboost" 
			"resultVar" "$tempvar" 
		}
		"Add"
		{
			"srcVar1"  "$tempvar" 
			"srcVar2"  "$basephongboost"
			"resultVar" "$phongboost" 
		}
	}
}